# Notion API
NOTION_API_KEY=your_notion_api_key_here
NOTION_PRODUCTS_DB_ID=your_products_database_id
NOTION_SERVICES_DB_ID=your_services_database_id
NOTION_CALENDAR_DB_ID=your_calendar_database_id
NOTION_PUBLICS_DB_ID=your_clients_database_id
NOTION_ECO_IMPACT_DB_ID=your_eco_impact_database_id

# Ollama Configuration
OLLAMA_API_ENDPOINT=http://127.0.0.1:11434/api
OLLAMA_DEFAULT_MODEL=codestral:latest
OLLAMA_MAX_TOKENS=2048
OLLAMA_TEMPERATURE=0.7

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Upload Configuration
UPLOAD_MAX_SIZE=5242880  # 5MB in bytes
ALLOWED_FILE_TYPES=.txt,.md,.json,.csv,.pdf
UPLOAD_DIR=public/uploads
